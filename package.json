{
  "author": "Tobias Kopelke <nox@raynode.de>",
  "dependencies": {
    "@types/jest": "^22.0.0",
    "@types/sinon": "^4.0.0",
    "sinon": "^4.1.2"
  },
  "description": "",
  "devDependencies": {
    "@types/faker": "^4.1.2",
    "@types/node": "^9.3.0",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cracks": "^3.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "faker": "^4.1.0",
    "jest": "^22.0.3",
    "jest-cli": "^22.0.5",
    "semantic-release": "^11.0.2",
    "sinon": "^4.1.2",
    "ts-jest": "^22.0.0",
    "ts-node": "^4.0.0",
    "typescript": "^2.6.1"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "logging",
    "meta-logging"
  ],
  "license": "MIT",
  "main": "dist/log.js",
  "name": "@raynode/nx-logger",
  "repository": {
    "type": "git",
    "url": "https://github.com/raynode/nx-logger"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "tslint 'lib/**/*.ts'",
    "lint:fix": "tslint --fix 'lib/**/*.ts'",
    "build": "rm -rf dist && tsc -p tsconfig.release.json",
    "test": "npm run test:coverage",
    "test-only": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "semantic-release": "semantic-release",
    "publish": "npm publish --access public",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "types": "dist/log.d.ts",
  "version": "1.0.16",
  "release": {
    "verifyRelease": {
      "path": "cracks",
      "paths": [
        "__tests__",
        "package.json"
      ]
    }
  }
}
